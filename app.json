{
  "expo": {
    "name": "spotify-project",
    "slug": "spotify-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "spotify-project", 
    "redirectUri": "exp://192.168.1.6:8081/--/spotify-auth-callback",
